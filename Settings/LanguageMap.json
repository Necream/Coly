{
    "C++": {
        "needcompile": true,
        "extension": "cpp",
        "compilerun": "g++ -std=c++17 $ -o ^.exe && ^.exe",
        "run": "^.exe"
    },
    "Python": {
        "needcompile": false,
        "extension": "py",
        "run": "py $"
    },
    "Coly": {
        "needcompile": false,
        "extension": "cly",
        "run": "coly $"
    }
}