{
    "C++": {
        "needcompile": true,
        "extension": "cpp",
        "compilerun": "cl /std:c++20 /utf-8 /EHsc $ && ^.exe",
        "run": "^.exe"
    },
    "Python": {
        "needcompile": false,
        "extension": "py",
        "run": "py $"
    },
    "Coly": {
        "needcompile": false,
        "extension": "cly",
        "run": "coly $"
    }
}